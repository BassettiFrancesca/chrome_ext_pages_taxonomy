{
  "name": "Pages taxonomy",
  "description": "A taxonomy of pages",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage", "tabs", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/tx_icon16.png",
      "32": "/images/tx_icon32.png",
      "48": "/images/tx_icon48.png",
      "128": "/images/tx_icon128.png"
    }
  },
  "icons": {
    "16": "/images/tx_icon16.png",
    "32": "/images/tx_icon32.png",
    "48": "/images/tx_icon48.png",
    "128": "/images/tx_icon128.png"
  }
}